{
  "name": "NadiesLee",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "xo": {
    "extends": "xo-react",
    "space": true,
    "semicolon": false,
    "rules": {
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error",
            "info"
          ]
        }
      ],
      "max-len": [
        "error",
        80
      ],
      "func-names": [
        "warn",
        "always"
      ],
      "capitalized-comments": [
        "off"
      ],
      "import/prefer-default-export": [
        "off",
        "always"
      ],
      "import/no-unassigned-import": [
        "warn"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "new-cap": [
        "error",
        {
          "capIsNew": false
        }
      ],
      "max-params": [
        "error",
        8
      ],
      "unicorn/filename-case": [
        "off"
      ],
      "react/jsx-space-before-closing": [
        "error",
        "always"
      ],
      "react/jsx-tag-spacing": [
        "error",
        {
          "closingSlash": "never",
          "beforeSelfClosing": "always",
          "afterOpening": "never"
        }
      ],
      "react/jsx-sort-props": [
        "off"
      ],
      "react/forbid-component-props": [
        "off"
      ],
      "react/no-string-refs": [
        "off"
      ],
      "react/jsx-no-bind": [
        "off"
      ]
    },
    "ignore": [
      "android",
      "ios",
      "lib/content/**/*"
    ]
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "dependencies": {
    "global": "^4.3.2",
    "native-base": "^2.1.3",
    "react": "16.0.0-alpha.6",
    "react-native": "0.43.4",
    "react-native-code-push": "^2.0.2-beta",
    "react-native-sqlite-storage": "^3.3.1",
    "react-native-vector-icons": "^4.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "19.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "1.9.1",
    "babel-register": "^6.24.1",
    "cheerio": "^0.22.0",
    "dotenv": "^4.0.0",
    "eslint-config-xo-react": "^0.11.1",
    "eslint-plugin-react": "^6.10.3",
    "front-matter": "^2.1.2",
    "jest": "19.0.2",
    "json2yaml": "^1.1.0",
    "lodash": "^4.17.4",
    "react-test-renderer": "16.0.0-alpha.6",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "runjs": "^3.2.1",
    "sort-keys": "^1.1.2",
    "string": "^3.3.3",
    "xo": "^0.18.1",
    "yamljs": "^0.2.10"
  },
  "jest": {
    "preset": "react-native"
  }
}
